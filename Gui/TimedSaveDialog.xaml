<Window x:Class="CDFM.Gui.TimedSaveDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Unsaved Settings" Height="121" Width="439">
    <Grid>
        <Button Name="buttonSave" Content="Save" HorizontalAlignment="Left" Margin="30,44,0,0" VerticalAlignment="Top" Width="75" Click="buttonSave_Click"/>
        <Button Name="buttonSaveAs" Content="Save As" HorizontalAlignment="Left" Margin="130,44,0,0" VerticalAlignment="Top" Width="75" Click="buttonSaveAs_Click"/>
        <Button Name="buttonDontSave" Content="Discard" HorizontalAlignment="Left" Margin="230,44,0,0" VerticalAlignment="Top" Width="75" Click="buttonDontSave_Click"/>
        <Button Name="buttonDisable" Content="Disable" HorizontalAlignment="Left" Margin="330,44,0,0" VerticalAlignment="Top" Width="75" Click="buttonDisable_Click"/>
        <Label Content="Settings have been changed but not saved." HorizontalAlignment="Left" Margin="24,10,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
